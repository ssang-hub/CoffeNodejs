<nav aria-label="Page navigation example" style="display:flex; margin: 50px auto;">
    <ul class="pagination" style="margin: auto ;">

      <!-- Previouts Page -->
    <% if(pageSelect!=1){ %>
      <li class="page-item"><a class="page-link"  href="/page/<%=pageSelect-1%>">Previous</a></li>
    <%}%>
    <% if(pageSelect==1){ %>
      <li class="page-item disabled">
        <a class="page-link" href="#" tabindex="-1">Previous</a>
      </li>
    <%}%>


    <!-- Select Page -->
    <% for(let i=1; i <= length; i++) { %>
      <% if(i==pageSelect) {%>
      <li class="page-item active"><a class="page-link" href="/page/<%=i%>"><%= i%></a></li>
      <%} else{%>
      <li class="page-item"><a class="page-link" href="/page/<%=i%>"><%= i%></a></li>
      <%}%>
    <% } %>  


    <!-- Next Page -->
    <% if(pageSelect < length){ %>
      <li class="page-item" ><a class="page-link"  href="/page/<%=pageSelect+1%>">Next</a></li>
    <%}%>
    <% if(pageSelect==length){ %>
      <li class="page-item disabled">
        <a class="page-link" href="#" tabindex="-1">Next</a>
      </li>
    <%}%>

    </ul>
</nav>