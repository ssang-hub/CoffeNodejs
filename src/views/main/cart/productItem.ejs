
<div class="card mb-3">
    <div class="card-body" style="width: 550px;">
    <div class="d-flex justify-content-between">
        <div class="d-flex flex-row align-items-center">
            <div>
                <img
                src="<%=data.image%>"
                class="img-fluid rounded-3 mr-3" alt="Shopping item" style="width: 65px;">
            </div>
            <div class="ms-3">
                <h5><%=data.name%></h5>
                <p class="small mb-0">size:<%=data.size%> , <%=data.note%></p>
            </div>
        </div>
        <div class="d-flex flex-row align-items-center ml-3">
                <div style="width: 50px;">
                    <h5 class="fw-normal mb-0"><%=data.number%></h5>
                </div>
                <div style="width: 80px;">
                    <h5 class="mb-0"><%=data.price%>Ä‘</h5>
                </div>
                 
                <button data-cartId="<%=data.id%>" type="submit" class="btn btn-outline-success delete-cart" style="color: #cecece;" class="p-2">
                    <i class="fas fa-trash-alt"></i>
                </button>
            </div>
     </div>
    </div>
</div>
